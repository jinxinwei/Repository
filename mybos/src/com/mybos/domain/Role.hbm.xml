<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mybos.domain">
<class name="Role" table="Role">
	<id name="id" type="java.lang.Long">
	<column name="id" not-null="true"></column>
	<generator class="native"></generator>
	</id>
	<property name="name" type="java.lang.String">
	<column name="name" not-null="true" length="20"></column>
	</property>
	<property name="description" type="java.lang.String">
	<column name="description" not-null="false" length="80"></column>
	</property>
	<set name="users" table="user_role" lazy="true" inverse="true">
		<key column="role_id"></key>
		<many-to-many column="user_id" class="User"></many-to-many>
	</set>
</class>
</hibernate-mapping>
