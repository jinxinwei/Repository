<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mybos.domain">
<class name="Standard" table="Standard">

	<id name="id" type="java.lang.Long">
	<column name="id" not-null="true"></column>
	<generator class="native"></generator>
	</id>
	<property name="name" type="java.lang.String">
	<column name="name" not-null="true" length="30" unique="true"></column>
	</property>
	
	<property name="minweight" type="java.lang.Double">
	<column name="minweight" not-null="true"></column>
	</property>

	<property name="maxweight" type="java.lang.Double">
	<column name="maxweight" not-null="true"></column>
	</property>

	<property name="deltag" type="boolean">
	<column name="deltag" not-null="true"></column>
	</property>

	<many-to-one name="user" class="User" not-null="true" column="user_id"></many-to-one>
	
	<property name="updatetime" type="timestamp">
	<column name="updatetime" not-null="true"></column>
	</property>
	
	<set name="staffs" lazy="true">
		<key column="standard_id"></key>
		<one-to-many class="Staff"/>
	</set>

</class>
</hibernate-mapping>
