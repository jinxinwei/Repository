<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mybos.domain">
<class name="Staff" table="Staff">
	<id name="id" type="java.lang.Long">
	<column name="id" not-null="true"></column>
	<generator class="native"></generator>
	</id>
	<property name="name" type="java.lang.String">
	<column name="name" not-null="true" length="30" unique="true"></column>
	</property>
	
	<property name="telephone" type="java.lang.String">
	<column name="telephone" not-null="true" length="12"></column>
	</property>
	
	<property name="haspda" type="boolean">
	<column name="haspda" not-null="true"></column>
	</property>

	<property name="deltag" type="boolean">
	<column name="deltag" not-null="true"></column>
	</property>
	
	<property name="station" type="java.lang.String">
	<column name="station" not-null="true"></column>
	</property>
	
	<many-to-one name="standard" class="Standard" column="standard_id"></many-to-one>
	
	<set name="decidedzones" lazy="true">
		<key column="staff_id"></key>
		<one-to-many class="Decidedzone"/>
	</set>
</class>
</hibernate-mapping>
